echo "Starting uWSGI and serving Django app in deploy mode ..."
touch $OPENSHIFT_REPO_DIR/DEPLOY
$OPENSHIFT_DATA_DIR/bin/uwsgi -s $OPENSHIFT_DIY_IP:$OPENSHIFT_DIY_PORT --socket-protocol http --pp $OPENSHIFT_REPO_DIR/ --module karajlug_org.wsgi -d $OPENSHIFT_DIY_LOG_DIR/app.log --pidfile $OPENSHIFT_TMP_DIR/uwsgi.pid